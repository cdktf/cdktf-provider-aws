    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "json-schema": "^0.2.5",
    "jest": "^26.0.1",
    "@types/jest": "^26.0.7",
    "ts-jest": "^26.1.0",
    "@types/node": "^10.17.0",
    "jsii": "^1.9.0",
    "jsii-diff": "^1.9.0",
    "jsii-pacmak": "^1.9.0",
    "jsii-release": "^0.1.6",
